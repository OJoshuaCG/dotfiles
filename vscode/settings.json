{
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[markdown]": {
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.black-formatter",
		"editor.formatOnSave": true
	},
	"breadcrumbs.enabled": true,
	"code-runner.executorMap": {
		"bat": "cmd /c",
		"c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"csharp": "dotnet run",
		"dart": "dart",
		"fsharp": "fsi",
		"go": "go run",
		"java": "cd $dir && javac $fileName && clear && java $fileNameWithoutExt ",
		"javascript": "node",
		"lua": "lua",
		"objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
		"php": "php",
		"powershell": "powershell -ExecutionPolicy ByPass -File",
		"python": "python -u",
		"r": "Rscript",
		"rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
		"sass": "sass --style expanded",
		"scheme": "csi -script",
		"scss": "scss --style expanded",
		"shellscript": "bash",
		"swift": "swift",
		"typescript": "ts-node",
		"vbscript": "cscript //Nologo"
	},
	"code-runner.runInTerminal": true,
	"cSpell.enabled": false,
	"cSpell.enableFiletypes": [
		"!jsonc"
	],
	"cSpell.userWords": [
		"Arch",
		"pacman",
		"postgres",
		"postgresql",
		"Postgresql"
	],
	"dateTime.showDayOfMonth": true,
	"dateTime.showMonth": true,
	"dimmer.opacity": 30,
	"disable-hardware-acceleration": true,
	"dotnet.codeLens.enableReferencesCodeLens": false,
	"editor.bracketPairColorization.enabled": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.organizeImports": "explicit"
	},
	"editor.cursorBlinking": "expand",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.fontFamily": "Source Code Pro, Geist Mono, Cascadia Code, JetBrains Mono, Fira Code",
	"editor.fontLigatures": true,
	"editor.formatOnSave": false,
	"editor.glyphMargin": true,
	"editor.guides.bracketPairs": "active",
	"editor.linkedEditing": true,
	"editor.minimap.enabled": false,
	"editor.mouseWheelZoom": true,
	"editor.stickyScroll.enabled": true,
	"editor.tabSize": 4,
	"editor.wordWrap": "wordWrapColumn",
	"emmet.includeLanguages": {
		"javascript": "html",
		"javascriptreact": "html",
		"php": "html",
		"xml": "html"
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/node_modules": true,
		"*.meta": true,
		"*.sln": true
	},
	"keyboard.dispatch": "keyCode",
	"lifeline.battery.interval": 10000,
	"lifeline.clock.format": "DD MMM h:mm A",
	"markdown.extension.print.theme": "dark",
	"prettier.singleQuote": false,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"search.exclude": {
		"**/*.code-search": true,
		"**/bower_components": true,
		"**/node_modules": true,
		"**/venv": true
	},
	"security.workspace.trust.untrustedFiles": "open",
	"tabout.disableByDefault": false,
	"telemetry.telemetryLevel": "off",
	"terminal.integrated.defaultProfile.linux": "bash",
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"terminal.integrated.fontFamily": "Hack Nerd Font, Hack NF",
	"terminal.integrated.fontSize": 14,
	"trailing-spaces.trimOnSave": true,
	"update.showReleaseNotes": false,
	"window.openFilesInNewWindow": "off",
	"window.openFoldersInNewWindow": "on",
	"window.restoreWindows": "none",
	"window.zoomLevel": -1.00,
	"workbench.activityBar.location": "default",
	"workbench.colorCustomizations": {
		"editorIndentGuide.activeBackground1": "#ffffff",
		"tab.activeBackground": "#3C3C3C",
		"tab.activeForeground": "#ffffff"
	},
	"workbench.colorTheme": "Catppuccin Macchiato",
	"workbench.iconTheme": "icons",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none"
}